{
  "name": "mobx-rest-axios-adapter",
  "version": "1.0.0",
  "description": "Axios adapter for mobx-rest",
  "repository": {
    "type": "git",
    "url": "git@github.com:IranTip/mobx-rest-axios-adapter.git"
  },
  "license": "MIT",
  "jest": {
    "testRegex": "/__tests__/.*\\.spec\\.js$"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "it",
      "describe",
      "beforeEach",
      "expect",
      "Class",
      "jest",
      "FormData",
      "File",
      "fail"
    ]
  },
  "dependencies": {
    "axios": "^0.16.2",
    "axios-mock-adapter": "^1.9.0",
    "lodash": "^4.17.4",
    "qs": "^6.5.1"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.9.0",
    "flow-bin": "^0.38.0",
    "jest": "^18.1.0",
    "snazzy": "^6.0.0",
    "standard": "^8.6.0"
  },
  "main": "lib",
  "scripts": {
    "compile": "./node_modules/.bin/babel src --out-dir lib",
    "prepare": "npm run compile",
    "jest": "BABEL_ENV=test NODE_PATH=src jest --no-cache",
    "lint": "standard --verbose | snazzy",
    "flow": "flow",
    "test": "npm run flow && npm run lint && npm run jest"
  }
}
